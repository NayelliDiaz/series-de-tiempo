#2005
informal105 <- subset(sdemt105,ENT==14 & TUE2==5)# TUE2==5 - sector informal
informals105<-nrow(informal105)
pobocups105<- nrow(pobocup105)
til1105<-(informals105/pobocups105)*100
til1105 

